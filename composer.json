{
    "name": "agenciafmd/starter",
    "type": "project",
    "description": "AgÃªncia F&MD - Laravel 11",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "repositories": {
        "agenciafmd/frontend": {
            "type": "path",
            "url": "packages/agenciafmd/frontend",
            "options": {
                "symlink": true
            }
        }
    },
    "require": {
        "php": "^8.2",
        "agenciafmd/frontend": "v11.x-dev",
        "agenciafmd/admix": "v11.x-dev",
        "agenciafmd/admix-leads": "v11.x-dev",
        "agenciafmd/admix-postal": "v11.x-dev",
        "agenciafmd/admix-ui": "v11.x-dev",
        "agenciafmd/laravel-support": "v11.x-dev",
        "laravel/framework": "^11.38",
        "laravel/tinker": "^2.10"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^3.5",
        "brianium/paratest": "^7.4",
        "fakerphp/faker": "^1.23",
        "larastan/larastan": "^2.9",
        "laravel/pint": "^1.20",
        "laravel/sail": "*",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.5",
        "nunomaduro/phpinsights": "^2.11",
        "pestphp/pest": "^2.35",
        "pestphp/pest-plugin-laravel": "^2.4",
        "roave/security-advisories": "dev-latest"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan vendor:publish --tag=admix-ui:views --ansi --force",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta",
            "@php artisan ide-helper:models 'packages/agenciafmd/*/src/Models' --nowrite"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "phpstan": "vendor/bin/phpstan analyse",
        "pint": "vendor/bin/pint packages -v"
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "pestphp/pest-plugin-laravel": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
