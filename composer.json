{
    "name": "agenciafmd/starter",
    "type": "project",
    "description": "AgÃªncia F&MD - Laravel 8",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "repositories": {
        "agenciafmd/frontend": {
            "type": "path",
            "url": "packages/agenciafmd/frontend",
            "options": {
                "symlink": true
            }
        }
    },
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "agenciafmd/admix": "dev-master",
        "agenciafmd/frontend": "*",
        "agenciafmd/laravel-sitemap": "dev-master",
        "agenciafmd/laravel-social-meta": "dev-master",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "laravel/framework": "^8.0",
        "laravel/horizon": "^5.7",
        "laravel/tinker": "^2.0",
        "spatie/laravel-robots-middleware": "^1.3"
    },
    "require-dev": {
        "facade/ignition": "^2.3.6",
        "friendsofphp/php-cs-fixer": "^2.16",
        "fakerphp/faker": "^1.9.1",
        "itsgoingd/clockwork": "^5.0",
        "laravel/envoy": "^2.4",
        "mixdinternet/homestead-ngrok": "dev-master",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^5.0",
        "nunomaduro/larastan": "^0.6",
        "phpunit/phpunit": "^9.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "Google\\Task\\Composer::cleanup",
            "@php artisan vendor:publish --force --tag=admix:assets --ansi",
            "@php artisan vendor:publish --force --tag=admix-analytics:assets --ansi",
            "@php artisan vendor:publish --force --tag=livewire:assets --ansi",
            "@php artisan horizon:publish --ansi"
        ],
        "phpstan": "vendor/bin/phpstan analyse"
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        },
        "google/apiclient-services": [
            "Analytics",
            "AnalyticsData",
            "AnalyticsReporting",
            "YouTube"
        ]
    }
}
